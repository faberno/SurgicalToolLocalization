{
    "train_dataset_params": {
        "dataset_name": "cholec80",
        "dataset_path": "/content/cholec80_sub",
        "set": "trainval",
        "loader_params": {
            "batch_size": 50,
            "shuffle": true,
            "num_workers": 2,
            "pin_memory": true
        },
        "transforms": {
            "toTensor": true,
            "resize": [336, 600],

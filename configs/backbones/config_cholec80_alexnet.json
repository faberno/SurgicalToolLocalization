            "masking": [0.5, [24, 24]],
            "rotation": 90,
            "normalize": true,
            "channelswitcher": false
        }
    },
    "val_dataset_params": {
        "dataset_name": "cholec80",
        "dataset_path": "/content/cholec80_sub",
        "set": "test",
        "loader_params": {
            "batch_size": 50,
            "shuffle": false,
            "num_workers": 2,
            "pin_memory": true
        },
        "transforms": {
            "toTensor": true,
            "resize": [336, 600],
            "normalize": true,
            "channelswitcher": false
        }
